package adivina;

public class Adivina {
	int rdm;
	int input;
	
	public Adivina(){
		rdm = 0;
		input = 0;
	}
	public Adivina(int random,int entrada){
		if(input<0 || input>100 )
			throw new IllegalArgumentException("Parametro invalido");
		else{

			input = entrada;
		}
		rdm = random;
	}
	
	public int num_rdm(){
		return rdm;
	}
	public int num_input(){
		return input;
	}
}
